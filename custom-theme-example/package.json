{
  "name": "custom-theme-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "format": "prettier . --write",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "agent": "agent-cli",
    "clean:icons": "npx @db-ux/icon-font-tools clean-icons --src ./public/assets/uncleaned-icons --out ./public/assets/icons",
    "generate:icon-font": "npx @db-ux/icon-font-tools generate-icon-fonts --src ./public/assets/icons --fontName thiemens"
  },
  "dependencies": {
    "@db-ux/react-core-components": "3.0.2-copilot2-e7bf98b",
    "@tailwindcss/vite": "^4.1.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "7.7.1"
  },
  "devDependencies": {
    "@db-ux/agent-cli": "3.0.2-copilot2-e7bf98b",
    "@db-ux/core-foundations": "3.0.2-copilot2-e7bf98b",
    "@db-ux/icon-font-tools": "0.3.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  }
}
